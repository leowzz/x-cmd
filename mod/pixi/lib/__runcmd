
___x_cmd_pixi___runmain(){
    ___x_cmd_pixi___runcmd "$@"
}

___x_cmd_pixi___runcmd(){
    if ___x_cmd_hascmd pixi; then
        ___x_cmd_pixi___runcmd(){  ___x_cmd_cmds pixi "$@"; }
    else
        ___x_cmd_pixi___install || return $?
        ___x_cmd_pixi___runcmd(){ "$___X_CMD_PIXI_BIN_PATH/$___X_CMD_PIXI_BIN" "$@" ; }
    fi || return $?
    ___x_cmd_pixi___runcmd "$@"
}
