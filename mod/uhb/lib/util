___x_cmd_uhb_vanilla(){
    ___x_cmd_cmds awk '
$0~/^#[ ]x-cmd/{
    exit
}
{
    print
}
'
}

___x_cmd_uhb___fzf(){
    FZF_DEFAULT_OPTS="
--ansi
--reverse
--height='80%'
--bind='ctrl-w:toggle-preview-wrap'
--bind='ctrl-r:change-preview-window(right,70%|down,40%,99%,border-horizontal|hidden|right)'
"   ___x_cmd fzf "$@"

}
