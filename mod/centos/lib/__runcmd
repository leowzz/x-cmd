
___x_cmd_centos___runmain(){
    ___x_cmd_centos___runcmd "$@"
}

___x_cmd_centos___runcmd(){
    if ___x_cmd_hascmd centos; then
        ___x_cmd_centos___runcmd(){  ___x_cmd_cmds centos "$@"; }
    else
        ___x_cmd_centos___install || return
        ___x_cmd_centos___runcmd(){ "$___X_CMD_centos_BIN_PATH/$___X_CMD_centos_BIN" "$@" ; }
    fi || return
    ___x_cmd_centos___runcmd "$@"
}
