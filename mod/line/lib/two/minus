

___x_cmd_line_minus(){
    case "$1" in
        -h|--help)      ___x_cmd help -m line   minus "$@"; return ;;
        --)             shift ;;
    esac

    local IFS="$___X_CMD_UNSEENCHAR_NEWLINE"

    data="$*" ___x_cmd_cmds_awk '
BEGIN{
    l = split(ENVIRON["data"], arr, "\n")
    for (i=1; i<=l; ++i) ban[arr[i]] = 1
}

ban[$0]!=1{  print;  }
'
}
