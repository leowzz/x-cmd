

___x_cmd_tsv_header(){
    local X_help_cmd='x help -m csv header'; help:arg-null:parse

    local op="$1"; shift
    case "$op" in
        --add)          ___x_cmd_tsv_header___add "$@"  ;;
        --mod)          ___x_cmd_tsv_header___mod "$@"  ;;
        *)              ___x_cmd_tsv_header_main "$op" "$@"   ;;
    esac

}

# Consider using awk ? ...
___x_cmd_tsv_header___add(){
    local IFS=" "
    local data="$___X_CMD_UNSEENCHAR_HT"
    printf "%s\n" "$*"
    ___x_cmd_cmds_cat
}

