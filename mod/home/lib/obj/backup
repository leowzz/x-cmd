
___x_cmd_home_obj_backup(){
    local fp="$1"
    ___x_cmd zuz z "$___X_CMD_HOME_DATA_ROOTFP" "$fp"
}

___x_cmd_home_obj_restore(){
    local fp="$1"
    local fpbak="${fp}.home.bak"
    (
        ___x_cmd mcd "${fpbak}"
        ___x_cmd zuz uz "$___X_CMD_HOME_DATA_ROOTFP" "$fpbak"
    )

    # restore fpbak to fp
}
