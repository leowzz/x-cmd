# NOTICE: unused
___x_cmd_env_appoint_candidate(){
    case $1 in
        USE) local pattern="USE"; shift ;;
    esac
    ___x_cmd pkg download "$@" || return 1
    local name="${1##*/}"
    local x_=; ___x_cmd_pkg_default_version_ "$name" || return
    if [ "$pattern" = "USE" ]; then
        ___x_cmd env use "$name=$x_"
    else
        ___x_cmd env try "$name=$x_"
    fi
}
