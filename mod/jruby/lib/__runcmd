
___x_cmd_jruby___runmain(){
    ___x_cmd_jruby___runcmd "$@"
}

___x_cmd_jruby___runcmd(){
    if ___x_cmd_hascmd jruby; then
        ___x_cmd_jruby___runcmd(){  ___x_cmd_cmds jruby "$@"; }
    else
        ___x_cmd_jruby___install || return
        ___x_cmd_jruby___runcmd(){ "$___X_CMD_jruby_BIN_PATH/$___X_CMD_jruby_BIN" "$@" ; }
    fi || return
    ___x_cmd_jruby___runcmd "$@"
}
